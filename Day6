int main() {
   int x,j;
    cin>>x;
    for(int i=0;i<x;i++){
        string s,s1,s2;
        cin>>s;
        for(j=0;j<s.size();j++){
            if(j%2==0){
                s1+=s[j];
            }
            else
                s2+=s[j];
        }
        cout<<s1<<" "<<s2<<endl;
    }
   
    return 0;
}
